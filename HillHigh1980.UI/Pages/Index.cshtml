@page
@using Newtonsoft.Json
@model IndexModel
@{
    ViewData["Title"] = "Harry Hill High School Class 1980";
    ViewData["IsDetailsView"] = false;
}

<partial name="_NavBar" model="true" />

<!-- Page Content -->
<section class="container">
    <h1 class="my-4 text-center text-lg-left">Lansing Hill High School Classmates 1980</h1>
    <h2 class="my-4 text-center text-lg-left">Lansing, Michigan</h2>

    <div id="showRoster" class="row text-center text-lg-left text-hide">
    </div>
</section>

@section Scripts {
    <environment include="Development">
        <script src="~/js/Bundles/indexView.js"></script>
    </environment>
    <environment exclude="Development">
        <script src="~/js/Bundles/indexView.min.js"></script>
    </environment>
    <script>
        var myObj = @Html.Raw(JsonConvert.SerializeObject(Model.Roster));
    </script>
}
